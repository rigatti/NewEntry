<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="db.family.FamilyLevel1" table="Familles_Niveau1" >
    	<id name="id">
    		<column name="CodeFamille1" length="1"/>
    	</id>
       	<property name="description">
       		<column name="DescriptionFamille1" length="50" />
       	</property>
       		<property name="descriptionShort">
       		<column name="DescrShortFamille1" length="20" />
       	</property>
       	<property name="margin">
       		<column name="Marge1" />
       	</property>
    </class>
</hibernate-mapping>
