<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="db.entry.treated.temp.TreatedEntryTemp" table="TreatedEntryTemp" >
       	<id name="id">
   			<column name="id" />
   			<generator class="native"/>
   		</id>
   		<property name="supplierCode"/>
       	<property name="arrivalDate"/>
       	<property name="arrivalTime"/>
       	<property name="productCode"/>
       	<property name="ean"/>
       	<property name="unitConditionnement"/>
       	<property name="numberOfUnit"/>
       	<property name="numberOfProduct"/>
       	<property name="lotNumber"/>
       	<property name="supplierOrderNumbers"/>
       	<property name="supplierOrderLetters"/>
       	<property name="validityDate"/>
       	<property name="additionalData"/>
       	<property name="description"/>
		<property name="numberOfProductReceived"/>
		<property name="supplierDocumentDescription"/>
		<property name="supplierDocumentType"/>
		<property name="stockMovement"/>
		<property name="supplierProductCode"/>
    </class>
</hibernate-mapping>
