<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="db.entry.treated.TreatedEntry" table="TreatedEntry" >
       	<id name="treatedEntryId"  column="id">
   			<generator class="native"/>
   		</id>
       	<property name="productCode"/>
   		<property name="validityDate"/>
       	<property name="ean"/>
       	<property name="lotNumber"/>
		<property name="supplierCode"/>
		<property name="supplierProductCode"/>
       	<property name="arrivalDate"/>
   		<property name="arrivalTime"/>
    </class>
</hibernate-mapping>
