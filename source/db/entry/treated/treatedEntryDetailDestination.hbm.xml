<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="db.entry.treated.TreatedEntryDetailDestination" table="TreatedEntryDetailDestination" >
    	<id name="id">
    		<column name="id"/>
    		<generator class="native"/>
    	</id>
       	<property name="treatedEntryDetailId"/>

       	<property name="allocationDate"/>

       	<property name="stockFlag"/>
       	<property name="supplierReturnsFlag"/>

       	<property name="customerCode"/>

   		<property name="numberOfUnit"/>
   		
   		<property name="packagingFromBasket"/>
   		<property name="packagingToBasket"/>
   		
   		<property name="airportCode"/>
   		<property name="flight"/>
   		<property name="lta"/>

   		<property name="substitutionOfProduct"/>
   		<property name="substitutionOfProductDescr"/>
   		<property name="substitutionOfUnitConditionnement"/>
   		<property name="substitutionOfNumberOfUnit"/>
   		<property name="substitutionOfNumberOfProduct"/>
   		<property name="substitutionOfCustomerOrderCode"/>
   		<property name="substitutionOfCustomerOrderNumber"/>
   		<property name="substitutionOfSupplierCode"/>
   		<property name="substitutionOfSupplierOrderLetter"/>
   		<property name="substitutionOfSupplierOrderNumber"/>
    </class>
</hibernate-mapping>
