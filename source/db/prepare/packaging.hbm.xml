<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="db.prepare.Packaging" table="PréparationColis" >
    	<composite-id>
    		<key-property name="orderNumber">
    			<column name="NuméroCommande"></column>
    		</key-property>
    		<key-property name="orderLetter">
    			<column name="LettreCommande" length="50"></column>
    		</key-property>
    		<key-property name="customerOrderCode">
    			<column name="CodeClientCommande"  length="5"></column>
    		</key-property>
    		<key-property name="customerOrderCodeNumber">
    			<column name="NuméroCodeClientCommande"></column>
    		</key-property>
    		<key-property name="feesType">
    			<column name="TypeFacture"></column>
    		</key-property>
    	</composite-id>
    	<property name="fromPackageNumber">
    		<column name="NuméroColisDébut" />
    	</property>
    	<property name="customerCode">
    		<column name="CodeClient"  length="12"></column>
    	</property>
    </class>
</hibernate-mapping>
