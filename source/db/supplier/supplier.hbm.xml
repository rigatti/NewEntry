<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="db.supplier.Supplier" table="Fournisseurs" >
    	<id name="supplierCode">
   			<column name="CodeFournisseur" length="2" />
   		</id>
       	<property name="description">
       		<column name="DescriptionFournisseur" length="50" />
       	</property>
    </class>
</hibernate-mapping>
